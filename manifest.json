{
	"name": "Youtube Video Search",
	"version": "1.0",
	"description": "Redirects the default googl videos tab to search youtube, instead of the default google videos search.",
	"manifest_version": 2,
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"matches": ["https://www.google.com/search*"],
			"js": ["jquery-3.5.1.min.js", "youtubevideosearch.js"]
		}
	],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": "icon48.png",
		"default_popup": "popup.html"
	}
}